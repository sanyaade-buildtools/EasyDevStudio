<Source>
	main_layout: main_layout
	other_scroll_view: other_scroll_view
	other_grid_layout: other_grid_layout
	panel_layout: panel_layout
	
    FloatLayout:
    	id: main_layout
        size: root.size
        
        # Easy Dev Studio Logo top-center
        Image:
            id: main_logo 
            source: 'atlas://images/eds/logo'
            pos_hint: {'x':.0, 'y':.440}
                   
        Button:
            id: back_btn
            background_color: (1, 1, 1, 0.7)
            text: 'Back'
            size_hint: (.20, .06)
            pos_hint: {'x':.01, 'y':.020}
            on_release: root.close_source_menu()
            
        Button:
            id: help_btn
            background_color: (1, 1, 1, 0.7)
            text: 'Help & FAQ'
            size_hint: (.20, .06)
            pos_hint: {'x':.790, 'y':.020}
            on_release: root.do_source_help_action() 

            
        BoxLayout:
        	id: source_box_layout
            orientation: "horizontal"
            pos_hint: {'x':.0, 'y':.15}
            padding: 10
         
			ScrollView:
		    	id: other_scroll_view
		    	size_hint: (None, None)
		    	do_scroll_x: False
		    	do_scroll_y: False # Make True To Enable Scrolling
		    	pos: (0, 0)
	            size: (250, 400)
	            bar_color: (47 / 255., 167 / 255., 212 / 255., 1.)
	            bar_width: 5
	            bar_margin: 30
	            
	            GridLayout:
	            	id: other_grid_layout
	            	cols: 1 
	            	spacing: 10
	            	size_hint: (None, None)
	                width: 300
	                # Set height over 400 to enable scrolling
	                height: 400  
	                #minimum_height: 400

			        Button:
			            id: Source_btn
			            background_color: (1, 1, 1, 0.7)
			            text: 'Build Rom From Source'
			            size_hint: (None, None)
			            size: (200, 40)
			            pos_hint: {'x':.350, 'y':.400}
			            on_release: root.do_source_action()
			            
			        Button:
			            id: kernel_btn
			            background_color: (1, 1, 1, 0.7)
			            text: 'Build Kernel From Source'
			            size_hint: (None, None)
			            size: (200, 40)
			            pos_hint: {'x':.350, 'y':.400}
			            on_release: root.do_kernel_action()
			                                
			                        
		    FloatLayout:
		    	id: panel_layout
	            
				Label:
					id: fastboot_note
					markup: True
					halign: 'center'
					text: "[b][color=#CC1100][size=15]Source Building Tools Are still under development\nand are Experimental\nUse Source building at your own Risk[/b][/size]"
					pos_hint: {'x':.0, 'y':.0}
					


